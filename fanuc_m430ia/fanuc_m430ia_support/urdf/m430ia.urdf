<?xml version="1.0" ?>
<robot name="fanuc_m430ia">

  <!-- ~~~~~~~~~~~~~ COLORS ~~~~~~~~~~~~~~ -->

  <material name="fanuc_grey">
    <color rgba="0.75 0.75 0.75 1.0"/>  
  </material> 


  <!-- ~~~~~~~~~~~~~ JOINTS ~~~~~~~~~~~~~~ -->


  <link name="base_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/m430ia/visual/base.stl" scale="0.1 0.1 0.1"/>
      </geometry>
      <material name="fanuc_grey"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/m430ia/visual/base.stl" scale="0.1 0.1 0.1"/>
      </geometry>
    </collision>
  </link>

  <link name="link_1">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/m430ia/visual/link_1.stl"/>
      </geometry>
      <material name="fanuc_grey"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/m430ia/collision/link_1.stl"/>
      </geometry>
    </collision>
  </link>

  <link name="link_2">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/m430ia/visual/link_2.stl"/>
      </geometry>
      <material name="fanuc_grey"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/m430ia/collision/link_2.stl"/>
      </geometry>
    </collision>
  </link>

  <link name="link_3">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/m430ia/visual/link_3.stl" scale="0.1 0.1 0.1"/>
      </geometry>
      <material name="fanuc_grey"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/m430ia/visual/link_3.stl" scale="0.1 0.1 0.1"/>
      </geometry>
    </collision>
  </link>

  <link name="link_4">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/m430ia/visual/link_4.stl"/>
      </geometry>
      <material name="fanuc_grey"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/m430ia/collision/link_4.stl"/>
      </geometry>
    </collision>
  </link>

  <link name="link_5">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/m430ia/visual/link_5.stl"/>
      </geometry>
      <material name="fanuc_grey"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/m430ia/collision/link_5.stl"/>
      </geometry>
    </collision>
  </link>


  <!-- ~~~~~~~~~~~~~ JOINTS ~~~~~~~~~~~~~~ -->


  <joint name="joint_1" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.475"/> 
    <parent link="base_link"/>
    <child link="link_1"/>
    <axis xyz="0 0 1"/>
    <limit effort="0" lower="-3.1415" upper="3.1415" velocity="5.24"/>
  </joint>

  <joint name="joint_2" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="link_1"/>
    <child link="link_2"/>
    <axis xyz="0 1 0"/>
    <limit effort="0" lower="-2.01" upper="2.01" velocity="5.59"/>
  </joint>

  <joint name="joint_3" type="revolute">
    <origin rpy="0 0 0" xyz="0 -0.095 0.350"/>
    <parent link="link_2"/>
    <child link="link_3"/>
    <axis xyz="0 1 0"/>
    <limit effort="0" lower="-3.34" upper="3.34" velocity="5.59"/>
  </joint>

  <joint name="joint_4" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.550"/>
    <parent link="link_3"/>
    <child link="link_4"/>
    <axis xyz="0 1 0"/>
    <limit effort="0" lower="-2.62" upper="2.62" velocity="6.28"/>
  </joint>

  <joint name="joint_5" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.065"/>
    <parent link="link_4"/>
    <child link="link_5"/>
    <axis xyz="0 0 1"/>
    <limit effort="0" lower="-4.71" upper="4.71" velocity="20.94"/>
  </joint>


  <!-- ~~~~~~~~~~~~~ CONNECT ~~~~~~~~~~~~~ -->


  <!-- Base link to work frame --> 
  <link name="base"/>
  <joint name="base_link-base" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.475"/>
    <parent link="base_link"/>
    <child link="base"/>
  </joint>

  <!-- Flange attachment point for EEF models --> 
  <link name="flange"/>
  <joint name="joint_5-flange" type="fixed">
    <origin rpy="0 -1.5707963267948966 0" xyz="0 0 0"/>
    <parent link="link_5"/>
    <child link="flange"/>
  </joint>

  <!-- Tool for all-zeros frame -->
  <link name="tool0"/>

  <!-- Used instead of flange becuase link is already in correct orientation and location -->
  <joint name="link_5-tool0" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="link_5"/>
    <child link="tool0"/>
  </joint>


  <!-- ~~~~~~~~~~~ END EFFECTOR ~~~~~~~~~~ -->


  <link name="ee_base">
    <visual>
      <origin rpy="0 1.5708 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/gripper/end_effector.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="fanuc_grey"/>
    </visual>
    <collision>
      <origin rpy="0 1.5708 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/gripper/end_effector.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <joint name="ee_base" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="flange"/>
    <child link="ee_base"/>
  </joint>

  <link name="left_finger">
    <visual>
      <origin rpy="-1.5708 0 -1.5708" xyz="0.015 0 0.01"/> 
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/gripper/finger.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="fanuc_grey"/>
    </visual>
    <collision>
      <origin rpy="-1.5708 0 -1.5708" xyz="0.015 0 0.01"/> 
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/gripper/finger.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <joint name="left_finger" type="prismatic">
    <limit lower="-0.005" upper="0.02" effort="0" velocity="5"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="ee_base"/>
    <child link="left_finger"/>
    <axis xyz="0 0 1"/>
  </joint>


  <link name="right_finger">
    <visual>
      <origin rpy="1.5708 0 1.5708" xyz="0.015 0 -0.01"/> 
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/gripper/finger.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="fanuc_grey"/>
    </visual>
    <collision>
      <origin rpy="1.5708 0 1.5708" xyz="0.015 0 -0.01"/> 
      <geometry>
        <mesh filename="package://fanuc_m430ia_support/meshes/gripper/finger.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <joint name="right_finger" type="prismatic">
    <limit lower="-0.02" upper="0.005" effort="0" velocity="5"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="ee_base"/>
    <child link="right_finger"/>
    <axis xyz="0 0 1"/>
  </joint>

</robot>